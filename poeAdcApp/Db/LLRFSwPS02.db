record(calc, "RA-RaSIA01:RF-LLRFSwPS:VoltPos5V-Mon"){
    field(CALC, "(5.*A/4095.)*(1.5)")
    field(INPA, "LLRFSwPS02:ADC0:Data-Mon.VAL[0] CP MSS")
    field(EGU, "V")
    field(DESC, "Tensão de saída da fonte de 5 Volts")
    field(DISV, "1")
    field(DISS, "INVALID")
    field(PREC, "4")
    field(SDIS, "LLRFSwPS02:ADC0:Data-Mon_enbl")
}

