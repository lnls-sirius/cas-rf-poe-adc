
record(stringin, "LLRFSwPS02:SaveName"){
    field(VAL,  "LLRFSwPS02.sav")
    field(PINI, "YES")
    field(DESC, "Autosave destination file")
}

record(waveform, "LLRFSwPS02:ADC0:Data-Mon"){
    field(PINI, "YES")
    field(DESC, "ADC0 Data")
    field(SCAN, "$(SCAN=1 second)")
    field(DTYP, "stream")
    field(INP,  "@ADC.proto getData(ADC0) $(PORT) $(A)")
    field(FTVL, "DOUBLE")
    field(NELM, "4")
}
record(calc, "LLRFSwPS02:ADC0:Data-Mon_enbl"){
   field(CALC, "A#0")
   field(INPA, "LLRFSwPS02:ADC0:Data-Mon.STAT CP")
   field(INPB, "LLRFSwPS02:ADC0:Data-Mon CP")
}
