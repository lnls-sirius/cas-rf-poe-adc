
record(calc, "$(P)$(R):ADC0:CH1:Voltage-Mon"){
    field(CALC, "(0*A/4095.)*((1.0+1.0)/1.0)")
    field(INPA, "$(P)$(R):ADC0:Data-Mon.VAL[0] CP MSS")
    field(EGU, "V")
    field(DESC, "")
}

