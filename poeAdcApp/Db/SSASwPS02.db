record(calc, "RA-RaSIA02:RF-SSASwPS:VoltPos5V-Mon"){
    field(CALC, "(5.*A/4095.)*(1.5)")
    field(INPA, "SSASwPS02:ADC0:Data-Mon.VAL[0] CP MSS")
    field(EGU, "V")
    field(DESC, "Tensão de saída da fonte de 5 Volts")
    field(MDEL, "0.001")
    field(DISV, "1")
    field(DISS, "INVALID")
    field(PREC, "4")
    field(SDIS, "SSASwPS02:ADC0:Data-Mon_enbl")
}

