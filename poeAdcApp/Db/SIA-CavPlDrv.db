
record(calc, "RA-RaSIA01:RF-CavPlDrivers:VoltPos5V-Mon"){
    field(CALC, "(5.*A/4095.)*(1)")
    field(INPA, "$(P)$(R):ADC0:Data-Mon.VAL[0] CP MSS")
    field(EGU, "V")
    field(DESC, "")
}

