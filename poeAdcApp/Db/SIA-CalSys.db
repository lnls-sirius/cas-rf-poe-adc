record(ao, "RA-RaSIA01:RF-LLRFCalSys:OFSdB1-Mon"){
    field(PREC, "2")
    field(EGU,  "dB")
    field(VAL,  "0")
    field(PINI, "YES")
}
record(calc, "RA-RaSIA01:RF-LLRFCalSys1_ADC"){
    field(CALC, "(5.*A/4095.)*(1)")
    field(INPA, "SIA-CalSys:ADC0:Data-Mon.VAL[0] CP MSS")
}
record(calc, "RA-RaSIA01:RF-LLRFCalSys:PwrdBm1_CALC"){
    field(CALC, "0*(F**4) + 0*(F**3) + 0*(F**2) + 1*F + 0")
    field(INPF, "RA-RaSIA01:RF-LLRFCalSys1_ADC CP MSS")
}
record(calc, "RA-RaSIA01:RF-LLRFCalSys:PwrdBm1-Mon"){
    field(CALC, "(A>-41.)?(A + B):(-Inf)")
    field(INPA, "RA-RaSIA01:RF-LLRFCalSys:PwrdBm1_CALC CP MSS")
    field(INPB, "RA-RaSIA01:RF-LLRFCalSys:OFSdB1-Mon CP MSS")
    field(PREC, "2")
    field(EGU,  "dBm")
}
record(calc, "RA-RaSIA01:RF-LLRFCalSys:PwrW1-Mon"){
    field(CALC, "(10**(A/10))*(1/1000)")
    field(INPA, "RA-RaSIA01:RF-LLRFCalSys:PwrdBm1-Mon CP MSS")
    field(PREC, "2")
    field(EGU,  "W")
}

