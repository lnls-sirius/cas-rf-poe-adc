record(waveform, "$(P)$(R)ADC0:Data-Mon"){
    field(PINI, "YES")
    field(DESC, "ADC0 Data")
    field(SCAN, "1 second")
    field(DTYP, "stream")
    field(INP,  "@SSABooster.proto getData(ADC0) $(PORT) $(A)")
    field(FTVL, "DOUBLE")
    field(NELM, "4")
}
